  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "dev": "tsc --watch",
    "lint": "eslint {src/*,test/*}.+(ts)",
    "format:check": "prettier --list-different \"{*,{src,test}/**/*}.+(ts)\"",
    "format:check:ci": "prettier --check --loglevel error \"{*,{src,test}/**/*}.+(ts)\"",
    "format:write": "prettier --write \"{*,{src,test}/**/*}.+(ts)\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "check": "npm run build:check && npm test && npm run lint && npm run format:check"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
